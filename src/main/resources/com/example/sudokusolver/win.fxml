<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" stylesheets="@../../../com.example.css/main.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sudokusolver.Controller.WinController">
   <children>
      <ImageView fitHeight="1080.0" fitWidth="1920.0" opacity="0.41" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../background.GIF" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0" spacing="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Winner" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <ImageView fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Icons/RoseStarNotCompleted.PNG" />
                           </image>
                        </ImageView>
                        <ImageView fx:id="starC1" fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Icons/RoseStarCompleted.PNG" />
                           </image>
                        </ImageView>
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <ImageView fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Icons/RoseStarNotCompleted.PNG" />
                           </image>
                        </ImageView>
                        <ImageView fx:id="starC2" fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Icons/RoseStarCompleted.PNG" />
                           </image>
                        </ImageView>
                     </children>
                  </StackPane>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <ImageView fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Icons/RoseStarNotCompleted.PNG" />
                           </image>
                        </ImageView>
                        <ImageView fx:id="starC3" fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../Icons/RoseStarCompleted.PNG" />
                           </image>
                        </ImageView>
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <Label fx:id="timeLabel" text="00:00:00" />
            <Button fx:id="finishButton" disable="true" mnemonicParsing="false" onAction="#finish" text="FINISH" />
         </children>
      </VBox>
   </children>
</AnchorPane>
